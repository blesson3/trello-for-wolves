{
  "name": "trello-for-wolves",
  "version": "1.0.0",
  "description": "Node.js wrapper for Trello API.",
  "main": "index.js",
  "repository": "https://github.com/mikerourke/trello-for-wolves",
  "author": "Mike Rourke <mike.w.rourke@gmail.com>",
  "license": "MIT",
  "scripts": {
    "doc:clean": "rimraf docs/",
    "doc:generate": "apidoc -i internals/api-docs/ -o docs/",
    "doc": "yarn run doc:clean && yarn run doc:generate",
    "flow": "flow check || exit 0",
    "lint": "eslint src --color || exit 0",
    "setup": "rm -rf node_modules && yarn cache clean && yarn install",
    "test": "cross-env NODE_ENV=test node internals/testing/run.js",
    "test:log": "cross-env LOG_TEST_RESULTS=true yarn test",
    "test:save": "cross-env SAVE_TEST_RESULTS=true yarn test"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "bluebird": "^3.5.0",
    "lodash.snakecase": "^4.1.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "chalk": "^1.1.3",
    "cross-env": "^5.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.48.0",
    "jsonfile": "^3.0.0",
    "mocha": "^3.4.2",
    "mocha-assume": "^1.0.0",
    "moment": "^2.18.1",
    "node-json-db": "^0.7.3",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.8"
  }
}
